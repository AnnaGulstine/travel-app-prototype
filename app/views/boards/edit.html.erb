  <section id="hero" class="module-hero module-video module-full-height bg-light-30" data-background="assets/images/section-11.jpg">
    <!-- HERO TEXT -->
    <div class="hero-caption">
      <div class="hero-text">
        <div class="row">
          <h3 class="mh-line-size-3 font-alt marginBottom newTitle" id="signUpTitle">Edit this Trip</h3>
        </div>

      <%= form_tag "/boards/#{@board.id}", method: :patch do %>

    <div ng-app="app" ng-controller="pinsController">   
 
      <div class="smallMarginTop">
        <%= text_field_tag :name, @board.name, class: "mh-line-size-4 addNewField", placeholder: "Location" %>
      </div>
      <div class="smallMarginTop newTitle">
        <h4 class="inline">EDIT COLORS</h4>
        <h4 class="inline">Light Gray</h4>
        <h4 class="inline">Purple</h4>
        <h4 class="inline">Black and Gray</h4>
      </div>
      <div class="smallMarginTop">
      <%= submit_tag 'Submit', class: "btn btn-default buttonMargin" %>
      <br />     
      <%= link_to "Delete This Board", "/boards/#{@board.id}", method: :delete, class: "btn btn-default smallMarginTop" %>
      </div>
      <% end %>
        </div>
      </div>
    </div> 
      <!-- YOUTUBE VIDEO BACKGROUND-->
      <div class="video-player" data-property="{videoURL:'/assets/oceanVideo.mp4', containment:'.module-video', quality:'large', startAt:0, autoPlay:true, loop:true, opacity:1, showControls:false, showYTLogo:false, vol:0, mute:true}"></div>
      <!-- /YOUTUBE VIDEO BACKGROUND-->
  </section>


